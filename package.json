{
  "name": "sample-wch-spa",
  "version": "6.0.0",
  "private": true,
  "description": "This project serves as a shell for WCH demo applications.",
  "bugs": {
    "url": "https://github.ibm.com/DX-Samples/sample-wch-spa/issues"
  },
  "license": "Apache-2.0",
  "author": "Carsten Leue <carsten.leue@de.ibm.com>",
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/DX-Samples/sample-wch-spa.git"
  },
  "scripts": {
    "build": "ng build --prod --source-map --vendor-source-map",
    "build:development": "ibm-wch-sdk-cli build development --relative",
    "build:production": "ibm-wch-sdk-cli build production --relative",
    "deploy": "npm-run-all build:production package:production deploy:purge deploy:push show:url",
    "deploy:ci": "npm run deploy",
    "deploy:purge": "ibm-wch-sdk-cli application purge",
    "deploy:push": "ibm-wch-sdk-cli application push --verbose --override --aggregated",
    "e2e": "ng e2e",
    "gen:components": "ibm-wch-sdk-cli create components --scss",
    "gen:layouts": "ibm-wch-sdk-cli create layouts",
    "gen:pagetype": "ibm-wch-sdk-cli create pagetype --scss --type",
    "gen:pull": "ibm-wch-sdk-cli application pull --data ./tmp/",
    "lint": "ng lint",
    "ng": "ng",
    "package:addBaseUrl": "ibm-wch-sdk-cli add base --relative",
    "package:addEsiPolyfill": "ibm-wch-sdk-cli add esi-polyfill",
    "package:addManifest": "ibm-wch-sdk-cli add webappmanifest --relative",
    "package:addPrefetch": "ibm-wch-sdk-cli add prefetch --relative",
    "package:compress": "ibm-wch-sdk-cli compress assets",
    "package:copyResources": "ibm-wch-sdk-cli copy build",
    "package:createSite": "ibm-wch-sdk-cli create site",
    "package:production": "npm run package:copyResources && npm run package:addPrefetch && npm run package:addBaseUrl && npm run package:addManifest && npm run package:addEsiPolyfill && npm run package:createSite",
    "run:wchtools": "ibm-wch-sdk-cli run wchtools",
    "show:types": "ibm-wch-sdk-cli show types",
    "show:url": "ibm-wch-sdk-cli show url",
    "start": "ng serve",
    "test": "ng test"
  },
  "config": {
    "data": "./data",
    "manifestId": "18e0e97e-7d32-4601-9ab8-655203f2c039",
    "name": "wch-spa-sample",
    "root": "/",
    "src": "./src"
  },
  "dependencies": {
    "@angular/animations": "next",
    "@angular/common": "next",
    "@angular/compiler": "next",
    "@angular/core": "next",
    "@angular/forms": "next",
    "@angular/http": "next",
    "@angular/platform-browser": "next",
    "@angular/platform-browser-dynamic": "next",
    "@angular/router": "next",
    "@ibm-wch-sdk/ng": "^7.0.0",
    "@ibm-wch-sdk/ng-edit": "^7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "next",
    "@angular/cli": "next",
    "@angular/compiler-cli": "next",
    "@angular/language-service": "next",
    "@ibm-wch-sdk/cli": "^7.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jslint": "^0.12.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "rxjs-tslint-rules": "^4.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  },
  "engines": {
    "node": ">= 8.0",
    "npm": ">= 5.0"
  }
}
